{
  "compilerOptions": {
    "jsx": "react",
    "esModuleInterop": true,
    "paths": {
      "@atoms/*": ["../../../atoms/*"],
      "@molecules/*": ["./molecules/*"],
      "@organisms/*": ["./organisms/*"],
      "@layouts/*": ["./layouts/*"],
      "@themes/*": ["./themes/*"]
    }
  },
  "extends": "tsconfig/react-library.json",
  "include": ["."],
  "exclude": ["dist", "build", "node_modules"]
}
