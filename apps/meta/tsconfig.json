{
  "extends": "tsconfig/nextjs.json",
  "include": [
    "next-env.d.ts",
    "next-auth.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@components/*": [
        "../../packages/cjbsDSTM/*"
      ]
    },
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules"
  ]
}
